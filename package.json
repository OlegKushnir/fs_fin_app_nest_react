{
  "name": "fs_fin_app_nest_react",
  "version": "1.0.0",
  "description": "fullstack app nestJS react",
  "scripts": {
    "build:backend": "cd nest-app && npm install && npm run build",
    "build:frontend": "cd react-app && npm install && npm run build",
    "build": "npm run build:backend & npm run build:frontend",
    "start:backend": "cd nest-app && npm run start",
    "start:frontend": "cd react-app &&  npm start",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OlegKushnir/fs_fin_app_nest_react.git"
  },
  "author": "Oleg Kushnir",
  "homepage": "https://github.com/OlegKushnir/fs_fin_app_nest_react#readme",
  "dependencies": {
    "concurrently": "^8.0.1"
  }
}